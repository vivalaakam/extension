{
  "version": "0.1.0",
  "name": "chrome-read",
  "description": "Chrome reading extension",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": ""
  },
  "manifest_version": 2
}
